{
  "version": "0.0.0",
  "name": "router.flow",
  "description": "Type-safe routing in flow",
  "keywords": [
    "Router.flow",
    "typed",
    "flow"
  ],
  "main": "lib/Router",
  "module": "src/Router",
  "author": "Irakli Gozalishvili <rfobic@gmail.com> (http://jeditoolkit.com)",
  "repository": "https://github.com/Gozala/router.flow",
  "license": "MIT",
  "devDependencies": {
    "alias-quokka-plugin": "^0.1.0",
    "babel-cli": "^6.26.0",
    "babel-preset-flow-node": "^1.0.2",
    "babel-register": "^6.26.0",
    "blue-tape": "^1.0.0",
    "documentation": "^5.0.1",
    "flow-bin": "^0.53.1",
    "flow-copy-source": "^1.2.1",
    "husky": "^0.14.0",
    "lint-staged": "^4.0.0",
    "prettier": "^1.4.4"
  },
  "scripts": {
    "test": "npm run test:flow && npm run test:tape",
    "test:tape": "blue-tape -r babel-register 'test/**/*.js'",
    "test:flow": "flow check",
    "build:clear": "rm -rf lib",
    "build:types": "flow-copy-source --verbose src lib",
    "build:node": "babel --out-dir lib src",
    "build:api": "documentation readme --section=API src/Router.js",
    "build:docs": "documentation build --document-exported src/** -f html --o docs",
    "build": "npm run build:node && npm run build:types",
    "prepublish": "npm run build && npm test",
    "precommit": "lint-staged",
    "start": "flow-copy-source --watch --verbose src lib & babel --watch --out-dir lib src"
  },
  "lint-staged": {
    "*.js": [
      "prettier --parser flow --no-semi --write",
      "git add"
    ]
  },
  "quokka": {
    "alias": {
      "router.flow": "src/Router"
    },
    "pro": true,
    "babel": true,
    "plugins": [
      "alias-quokka-plugin"
    ]
  },
  "dependencies": {
    "route.flow": "^0.1.3"
  }
}
